variant: nestos
version: 1.0.0
storage:
  files:
    - path: /etc/NetworkManager/system-connections/eth1.nmconnection
      mode: 0600
      contents:
        inline: |
          [connection]
          id=eth1
          type=ethernet
          interface-name=eth1
          [ipv4]
          dns-search=
          may-fail=false
          method=auto
